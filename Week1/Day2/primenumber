package week1.day2;

public class PrimeNumber {

	public static void main(String[] args) {
		int a = 13;
		boolean flag = false;
		for (int i = 2; i < a; i++) {
			if (a % i == 0) {
				flag = true;

			}

		}
		if (flag) {
			System.out.println("its a prime number");
		} else {
			System.out.println("its not a prime number");
		}
	}

}
