package week1.day2;

import java.util.Arrays;

public class array {
	public static void main(String[] args) {
		int[] number = { 3, 2, 11, 4, 6, 7 };

		Arrays.sort(number);
		for (int k = 0; k < number.length; k++) {
			System.out.println(number[k]);

		}
		System.out.println(number[number.length - 1]);

	}
}
